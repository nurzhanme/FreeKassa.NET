<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Nurzhan Aitbayev</Authors>
    <Description>FreeKassa api interaction on c#</Description>
    <PackageProjectUrl>https://github.com/nurzhanme/FreeKassa.NET</PackageProjectUrl>
    <RepositoryUrl>https://github.com/nurzhanme/FreeKassa.NET</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="6.0.0" />
  </ItemGroup>

</Project>
